# Production Environment Variables
REACT_APP_API_URL=https://tamarmyaybackend-last.onrender.com
REACT_APP_PRINT_SERVER_URL=http://localhost:3001

# Print Server Settings
PORT=3001
NODE_ENV=production

# Printer Configuration
PRINTER_TYPE=USB
# PRINTER_TYPE=SERIAL
# PRINTER_TYPE=NETWORK

# USB Printer Settings (most common)
USB_VENDOR_ID=0x04b8
USB_PRODUCT_ID=0x0202

# Serial Printer Settings (if using serial)
SERIAL_PORT=COM1
SERIAL_BAUDRATE=9600

# Network Printer Settings (if using network printer)
PRINTER_IP=192.168.1.100
PRINTER_PORT=9100
